{
  "name": "fog-compute",
  "version": "0.1.0",
  "description": "Unified Fog Compute Platform - BitChat, Tokenomics, and Edge Computing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:rust": "cd tests/rust && cargo test",
    "test:python": "pytest tests/python/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:mobile": "playwright test mobile.spec.ts",
    "test:browser": "playwright test cross-browser.spec.ts",
    "test:all": "npm run test && npm run test:rust && npm run test:python && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:rust && npm run test:python && playwright test --reporter=junit",
    "perf:system": "python tests/performance/system_test.py",
    "perf:network": "cd tests/performance && cargo run --bin network_test --release",
    "lint": "eslint src/**/*.{ts,tsx}",
    "typecheck": "tsc --noEmit",
    "dev": "tsc --watch",
    "benchmark": "cd src/fog && python benchmarks/run_benchmarks.py",
    "docker:up": "docker-compose -f docker-compose.betanet.yml up -d",
    "docker:down": "docker-compose -f docker-compose.betanet.yml down",
    "playwright:install": "npx playwright install",
    "coverage": "npm run test:coverage && pytest --cov=src/fog --cov-report=html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/fog-compute"
  },
  "keywords": [
    "fog-compute",
    "edge-computing",
    "p2p",
    "bitchat",
    "betanet",
    "privacy",
    "decentralized"
  ],
  "author": "Fog Compute Team",
  "license": "MIT",
  "dependencies": {
    "@react-three/drei": "^9.92.0",
    "@react-three/fiber": "^8.15.0",
    "events": "^3.3.0",
    "playwright": "^1.55.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simple-peer": "^9.11.1",
    "three": "^0.160.0",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/simple-peer": "^9.11.8",
    "@types/three": "^0.160.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
